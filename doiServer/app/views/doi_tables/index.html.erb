<h1>Listing doi_tables</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Desc</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doi_tables.each do |doi_table| %>
      <tr>
        <td><%= doi_table.name %></td>
        <td><%= doi_table.desc %></td>
        <td><%= doi_table.url %></td>
        <td><%= link_to 'Show', doi_table %></td>
        <td><%= link_to 'Edit', edit_doi_table_path(doi_table) %></td>
        <td><%= link_to 'Destroy', doi_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doi table', new_doi_table_path %>
